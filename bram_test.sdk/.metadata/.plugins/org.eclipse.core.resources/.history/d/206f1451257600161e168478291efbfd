#ifndef CLOSED_LOOP_H_
#define CLOSED_LOOP_H_


typedef struct Closed {

	float freq;
	int drive;

	float (*get_freq) (void *this);
	void (*set_drive) (void *this, int drive);



} Closed;



#endif /* CLOSED_LOOP_H_ */
